build --java_language_version=20
build --java_runtime_version=remotejdk_20
build --tool_java_language_version=20
build --tool_java_runtime_version=remotejdk_20

build --nojava_header_compilation
build --javacopt="-XepDisableAllChecks"

build --javacopt="--enable-preview"
build --javacopt="--add-modules=jdk.incubator.concurrent"
build --javacopt="--add-modules=jdk.incubator.vector"
build --jvmopt="--enable-preview"
build --jvmopt="--add-modules=jdk.incubator.concurrent"
build --jvmopt="--add-modules=jdk.incubator.vector"
